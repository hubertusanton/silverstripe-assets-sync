{
  "name": "webium/silverstripe-assets-sync",
  "description": "A SilverStripe module for importing files from a directory into the Assets system",
  "type": "silverstripe-vendormodule",
  "keywords": [
    "silverstripe",
    "assets",
    "import",
    "sync",
    "files"
  ],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Bart van Irsel",
      "email": "bart@webium.nl"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "silverstripe/framework": "^4.0 || ^5.0",
    "silverstripe/assets": "^1.0 || ^2.0"
  },
  "autoload": {
    "psr-4": {
      "Webium\\AssetsSync\\": "src/"
    }
  },
  "extra": {
    "installer-name": "assets-sync"
  },
  "minimum-stability": "stable"
}